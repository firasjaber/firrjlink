<h1>Show Link</h1>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @link.title %>
  </li>

  <li>
    <strong>Link:</strong>
    <%= @link.link %>
  </li>

  <li>
    <strong>Slug:</strong>
    <%= @link.slug %>
  </li>

  <li>
    <strong>Views:</strong>
    <%= @link.views %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.link_path(@conn, :edit, @link) %></span> |
<span><%= link "Back", to: Routes.link_path(@conn, :index) %></span>
